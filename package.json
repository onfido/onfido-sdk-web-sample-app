{
  "name": "onfido-sdk-ui-app",
  "version": "0.0.2",
  "description": "Sample App for JavaScript SDK view layer for Onfido identity verification",
  "scripts": {
    "prestart": "npm run build",
    "start": "babel-node server/server.js",
    "build": "NODE_ENV=production webpack -p --progress",
    "lint": "eslint {src,test}",
    "link:sdk": "npm link ../onfido-sdk-ui",
    "unlink:sdk": "rm -r ./node_modules/onfido-sdk-ui && npm install"
  },
  "keywords": [
    "onfido",
    "sdk",
    "document",
    "capture",
    "identity",
    "verification",
    "sample"
  ],
  "files": [
    "src"
  ],
  "license": "MIT",
  "author": "Ricardo Freitas <ricardo.freitas@onfido.com> (https://github.com/rfreitas)",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^1.0.8",
    "css-loader": "^2.1.1",
    "eslint": "^2.5.3",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.23.1",
    "url-loader": "0.5.8",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.4.4",
    "express": "^4.14.0",
    "onfido-sdk-ui": "^5.2.3",
    "pem": "^1.13.2",
    "unirest": "^0.5.1"
  }
}
